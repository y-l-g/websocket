{
    "name": "pogo/websocket",
    "description": "Pogo WebSocket Driver",
    "autoload": {
        "psr-4": {
            "Pogo\\WebSocket\\": "lib/src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pogo\\WebSocket\\ServiceProvider"
            ]
        }
    },
    "require": {
        "laravel/octane": "2.13",
        "pusher/pusher-php-server": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.4",
        "friendsofphp/php-cs-fixer": "^3.90",
        "phpstan/phpstan": "^2.1",
        "mockery/mockery": "^1.6"
    }
}
